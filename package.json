{
  "name": "@tosvenson/n8n-nodes-listmonk",
  "version": "1.0.27",
  "description": "n8n node for Listmonk API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://dotsndots.be",
  "author": {
    "name": "Sven Huijbrechts",
    "email": "sven.huijbrechts@dotsndots.be"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:toSvenson/n8n-nodes-listmonk.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPattern=e2e",
    "release": "n8n-node release",
    "format": "prettier nodes credentials --write"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/listmonkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/listmonk/listmonk.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "n8n-node-dev": "^1.0.0",
    "n8n-workflow": "*",
    "nock": "^14.0.10",
    "prettier": "^3.3.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.3",
    "yaml": "^2.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.0"
  }
}
